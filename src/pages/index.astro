---
import { getEntry } from 'astro:content';

import Layout from '@/layouts/Layout.astro';

const meta = await getEntry('site', 'meta');
---

<Layout
  title={meta.data.index.title}
  description={meta.data.index.description}
  path="/"
  og={{
    enabled: true,
  }}
>
  <main>
    <h1>フナイログ</h1>
  </main>
</Layout>
